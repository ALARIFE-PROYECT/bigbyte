{
  "name": "@bigbyte/ctx",
  "version": "0.1.0",
  "author": "Jose Eduardo Soria Garcia <alarifeproyect@gmail.com>",
  "license": "Apache 2.0",
  "private": false,
  "type": "commonjs",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "npx tsc",
    "yalc:install": "npx yalc add @bigbyte/utils && npx yalc add @bigbyte/ioc",
    "yalc:publish": "npm run build && npx yalc publish --push",
    "test": "npx tsc --project tsconfig.test.json && node --test dist/test"
  },
  "dependencies": {
    "@bigbyte/utils": "file:.yalc/@bigbyte/utils",
    "@bigbyte/ioc": "file:.yalc/@bigbyte/ioc",
    "tslib": "~2.3",
    "zone.js": "~0.14",
    "uuid": "~11.1"
  },
  "devDependencies": {
    "@types/node": "~22",
    "typescript": "~5.9"
  }
}
